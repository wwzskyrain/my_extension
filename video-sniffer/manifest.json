{
  "manifest_version": 3,
  "name": "Video URL Sniffer",
  "version": "0.1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "Extract Video",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "*://*.pornhub.com/*",
    "http://127.0.0.1/*",
    "http://localhost:8988/*"
  ],
  "permissions": [
    "tabs",
    "scripting"
  ]
}